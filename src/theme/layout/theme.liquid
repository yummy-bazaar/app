							{% for link in linklists[link.handle].links %}
							{% capture count %}{{ count }}*{% endcapture %}
							{% endfor %}
							{% endif %}
							{% endfor %}
							{% assign half_count = count | divided_by: 2 %}
							{% assign column_threshold = settings.dropdown_column_links | minus: 0 %}

