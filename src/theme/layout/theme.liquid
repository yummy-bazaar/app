							<ul>
							{% assign count = 0 %}
							{% assign split = false %}
							{% for link in linklists[link.handle].links %}
							{% capture count %}{{ count }}*{% endcapture %}
							<li><a href="{{ link.url }}" title="{{ link.title }}">{{ link.title }}</a></li>
